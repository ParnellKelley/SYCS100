def bsearch (element, alist):
	StartPoint = -1
	EndPoint = len(alist)
	while StartPoint != EndPoint-1:
		MidPoint = (StartPoint + EndPoint) / 2
		if element > alist[MidPoint]:
			StartPoint = MidPoint
		elif element < alist[MidPoint]:
			EndPoint = MidPoint
		elif element == alist[MidPoint]:
			return MidPoint
	return -1
